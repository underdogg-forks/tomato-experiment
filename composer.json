{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "replace": {
        "tomatophp/filament-accounts": "*",
        "tomatophp/filament-tenancy": "*",
        "tomatophp/filament-users": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "abraham/twitteroauth": "*",
        "ashallendesign/laravel-config-validator": "^2.2",
        "awcodes/filament-tiptap-editor": "^3.4",
        "baidouabdellah/laravel-arpdf": "dev-main",
        "bezhansalleh/filament-google-analytics": "^2.0",
        "bezhansalleh/filament-shield": "^3.2",
        "bugsnag/bugsnag-laravel": "^2.26.0",
        "filament/filament": "^3.2",
        "google/apiclient": "^2.17",
        "laravel/framework": "^11.9",
        "laravel/jetstream": "^5.1",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.15",
        "livewire/livewire": "^3.5",
        "mallardduck/blade-boxicons": "^2.4",
        "nwidart/laravel-modules": "^11.0",
        "openai-php/laravel": "^0.10.1",
        "revolution/socialite-discord": "^1.4",
        "socialiteproviders/apple": "^5.6",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/snapchat": "^4.1",
        "socialiteproviders/tiktok": "^5.2",
        "socialiteproviders/twitter": "^4.1",
        "spatie/laravel-markdown": "^2.5",
        "spatie/shiki-php": "^2.1",
        "stancl/tenancy": "^3.8",
        "stechstudio/filament-impersonate": "^3.13",
        "stichoza/google-translate-php": "^5.1",
        "tomatophp/filament-alerts": "^2.0",
        "tomatophp/filament-api": "*",
        "tomatophp/filament-cms": "*",
        "tomatophp/filament-discord": "*",
        "tomatophp/filament-docs": "*",
        "tomatophp/filament-ecommerce": "*",
        "tomatophp/filament-employees": "*",
        "tomatophp/filament-fcm": "*",
        "tomatophp/filament-invoices": "*",
        "tomatophp/filament-issues": "*",
        "tomatophp/filament-language-switcher": "*",
        "tomatophp/filament-logger": "*",
        "tomatophp/filament-media-manager": "*",
        "tomatophp/filament-menus": "*",
        "tomatophp/filament-notes": "*",
        "tomatophp/filament-payments": "*",
        "tomatophp/filament-pos": "*",
        "tomatophp/filament-pwa": "*",
        "tomatophp/filament-seo": "*",
        "tomatophp/filament-settings-hub": "*",
        "tomatophp/filament-subscriptions": "*",
        "tomatophp/filament-translations": "*",
        "tomatophp/filament-types": "*",
        "tomatophp/filament-wallet": "*",
        "tomatophp/filament-withdrawals": "*",
        "tomatophp/laravel-package-generator": "*",
        "vormkracht10/laravel-seo-scanner": "*",
        "ymigval/laravel-indexnow": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "driftingly/rector-laravel": "*",
        "fakerphp/faker": "*",
        "laravel/pint": "*",
        "laravel/sail": "*",
        "laravel/tinker": "*",
        "mockery/mockery": "*",
        "nunomaduro/collision": "*",
        "phpstan/phpstan": "*",
        "phpunit/phpunit": "^11.0",
        "rector/rector": "*",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "TomatoPHP\\FilamentTenancy\\": "app/TomatoPHP/FilamentTenancy/",
            "TomatoPHP\\FilamentAccounts\\": "app/TomatoPHP/FilamentAccounts/",
            "TomatoPHP\\FilamentUsers\\": "app/TomatoPHP/FilamentUsers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ]
        }
    }
}
